{
	"[typescript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint",
		"editor.formatOnSave": true
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint",
		"editor.formatOnSave": true
	},
	"eslint.run": "onType",
	"eslint.format.enable": true,
    "typescript.tsdk": "node_modules/typescript/lib",
    "robloxLsp.diagnostics.globals": [
        "Drawing",
        "Drawing",
        "Drawing"
    ],
    "robloxLsp.diagnostics.disable": [
        "undefined-global"
    ]
}